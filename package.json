{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "codegen": "graphql-codegen",
    "execute": "ts-node src/index.ts",
    "lint": "eslint --config ./.eslintrc.js ./src/ --ext .jsx,.js,.tsx,.ts",
    "lint:fix": "prettier --write ./src/ & yarn lint --fix",
    "terraform:init": "cd terraform && ./terraform_init.sh",
    "terraform": "terraform -chdir=terraform/",
    "build": "tsc",
    "predeploy": "yarn build && cp package.json build/ && cp yarn.lock build/",
    "deploy": "gcloud functions deploy token-holder-balances-$(git rev-parse --abbrev-ref HEAD) --gen2 --runtime=nodejs16 --region=us-east1 --source=build/ --entry-point=fetchScheduled --trigger-topic=token-holders-$(git rev-parse --abbrev-ref HEAD) --set-env-vars GCS_BUCKET=olympusdao-token-holders-$(git rev-parse --abbrev-ref HEAD) --max-instances 1 --memory=2GiB --timeout=540"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "@google-cloud/storage": "^6.5.2",
    "@urql/core": "^3.0.3",
    "big.js": "^6.2.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "jsonl-parse-stringify": "^1.0.3",
    "objects-to-csv": "^1.3.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.12.2",
    "@graphql-codegen/introspection": "^2.2.1",
